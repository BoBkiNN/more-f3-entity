{
  "schemaVersion": 1,
  "id": "f3-entity-ids",
  "version": "${version}",
  "name": "F3 Entity IDs",
  "description": "Adds additional line to right side for debug menu from BetterF3 mod.\nIt contains entity id and entity UUID separated by `/`",
  "authors": ["BoBkiNN"],
  "contact": {
    "homepage": "https://modrinth.com/mod/f3-entity-ids",
    "sources": "https://github.com/BoBkiNN/more-f3-entity",
    "issues": "https://github.com/BoBkiNN/more-f3-entity/issues"
  },
  "license": "MIT",
  "icon": "assets/f3-entity-ids/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xyz.bobkinn.moreF3Entity.client.MoreF3EntityClient"
    ]
  },
  "mixins": [
    "f3-entity-ids.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "betterf3": "*"
  }
}
